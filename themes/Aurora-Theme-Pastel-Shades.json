        "settings": {
          "foreground": "#FFD700"
        }
      },
      {
        "scope": "variable.other.local",
        "settings": {
          "foreground": "#F79C42"
        }
      },
      {
        "scope": "comment",
        "settings": {
          "foreground": "#8A8A8A",
